<%= form_with model: @school, url: schools_path, local: true do |f| %>
  <%= f.text_field :school_name %>
  <%= f.text_field :email %>
  <%= f.password_field :password %>
  <%= f.password_field :password_confirmation %>

  <%= f.text_field :teacher_password %>
  <%= f.text_field :student_password %>
  <%= f.submit 'Create Shool' %>
<% end %>